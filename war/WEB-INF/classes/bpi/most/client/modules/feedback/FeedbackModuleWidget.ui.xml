<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:my="urn:import:bpi.most.client.utils.ui"
	xmlns:myd="urn:import:bpi.most.client.utils.ui">

	<ui:style>
		.re {
			background-color: red;
			width: 400px;
			height: 1000px;
		}
	</ui:style>
	<g:FormPanel ui:field="form">
			
	<g:AbsolutePanel ui:field="mPMmPanel"
		styleName="absolute-panel absolute-panel-person">
		
		
				
				<myd:GeneralDropWidget ui:field="dropField1"
					styleName="dropWidget dropWidget-personModule red floatLeft dWidget-uid-person">
					
					<myd:draggable>

						<myd:GeneralDragWidget ui:field="drag5"
							styleName="dragWidget dWidget-uid-person dWidget-uid-desktop">
							<myd:header>
								<g:HorizontalPanel styleName="dragWidget-horizontal-panel">
									<g:Label styleName="label-icon-person-information"></g:Label>
									<g:Label styleName="label-tag-airmovement label-person-airmovement">New working place protocol</g:Label>
								</g:HorizontalPanel>
							</myd:header>
							<myd:content>
							<g:FlowPanel styleName="flow-panel flow-panel-content">
							
								
								<g:Label styleName="label">
									The desktop module is thought as a feedback module from the people
									working/living
									in the monitored building. Because of the questionnaire the PMV will
									be calculated.
									This is just a demo where no functionality is built in.
								</g:Label>
								</g:FlowPanel>
							</myd:content>
						</myd:GeneralDragWidget>
					</myd:draggable>
					
					
					<myd:draggable>

						<myd:GeneralDragWidget ui:field="drag1"
							styleName="dragWidget dWidget-uid-person dWidget-uid-desktop">
							<myd:header>
								<g:HorizontalPanel styleName="dragWidget-horizontal-panel">
									<g:Label styleName="label-icon-airmovement"></g:Label>
									<g:Label styleName="label-tag-airmovement label-person-airmovement">Air movement</g:Label>
								</g:HorizontalPanel>
							</myd:header>
							<myd:content>
								<g:FlowPanel
									styleName="flow-panel flow-panel-content flow-panel-airmovement">

									<g:FlowPanel styleName="flow-panel flow-panel-room">
										<g:Label>I can currently feel the air:</g:Label>
										<g:RadioButton name="Luft" formValue="stark spüren">very strong</g:RadioButton>
										<g:RadioButton name="Luft" formValue="deutlich spüren">clearly
											strong</g:RadioButton>
										<g:RadioButton name="Luft" formValue="spüren">ok</g:RadioButton>
										<g:RadioButton name="Luft" formValue="kaum spüren">hardly</g:RadioButton>
										<g:RadioButton name="Luft" formValue="nicht spüren">not at all</g:RadioButton>
									</g:FlowPanel>
									<g:FlowPanel styleName="flow-panel flow-panel-cloth">
										<g:Label>I think currently the air movement is:</g:Label>
										<g:RadioButton name="Luftbewegung" formValue="sehr angenehm">very
											pleasant</g:RadioButton>
										<g:RadioButton name="Luftbewegung" formValue="angenehm">pleasant</g:RadioButton>
										<g:RadioButton name="Luftbewegung" formValue="neutral">neutral</g:RadioButton>
										<g:RadioButton name="Luftbewegung" formValue="unangenehm">unpleasant</g:RadioButton>
										<g:RadioButton name="Luftbewegung" formValue="sehr unangenehm">very
											unpleasant</g:RadioButton>
									</g:FlowPanel>
								</g:FlowPanel>
							</myd:content>
						</myd:GeneralDragWidget>
					</myd:draggable>


					<myd:draggable>
						<myd:GeneralDragWidget ui:field="drag2"
							styleName="dragWidget dWidget-uid-person">
							<myd:header>
								<g:HorizontalPanel styleName="dragWidget-horizontal-panel">
									<g:Label styleName="label-icon-airtemp"></g:Label>
									<g:Label styleName="label-tag-airtemp label-person-airtemp">Air temperature</g:Label>
								</g:HorizontalPanel>
							</myd:header>
							<myd:content>
								<g:FlowPanel styleName="flow-panel flow-panel-content flow-panel-airtemp">

									<g:FlowPanel styleName="flow-panel flow-panel-room"
										ui:field="temperatur">

										<g:Label>I think the room is:</g:Label>
										<g:RadioButton name="Temperatur" formValue="heiss">hot</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="warm">warm</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="etwas warm">quite
											warm</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="neutral">neutral</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="etwas kühl">quite
											cool</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="kühl">cool</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="kalt">cold</g:RadioButton>
									</g:FlowPanel>
									<g:FlowPanel styleName="flow-panel flow-panel-cloth"
										ui:field="bekleidung">
										<g:Label>My clothes are:</g:Label>
										<g:RadioButton name="Temperatur" formValue="heiss">light</g:RadioButton>
										<g:RadioButton name="Temperatur" formValue="warm">middle</g:RadioButton>
										<g:RadioButton name="Temperatur " formValue="etwas warm">heavy</g:RadioButton>

									</g:FlowPanel>
								</g:FlowPanel>
							</myd:content>
						</myd:GeneralDragWidget>

					</myd:draggable>


				</myd:GeneralDropWidget>
				<myd:GeneralDropWidget ui:field="dropField2"
					styleName="dropWidget dropWidget-personModule red floatLeft dWidget-uid-person ">
					<myd:draggable>
						<myd:GeneralDragWidget ui:field="drag3"
							styleName="dragWidget dWidget-uid-person">
							<myd:header>
								<g:HorizontalPanel styleName="dragWidget-horizontal-panel">
									<g:Label styleName="label-icon-activity"></g:Label>
									<g:Label styleName="label-tag-activity label-person-activity">Personal activity</g:Label>
								</g:HorizontalPanel>
							</myd:header>
							<myd:content>

								<g:FlowPanel styleName="flow-panel flow-panel-content flow-panel-activity">

									<g:FlowPanel styleName="flow-panel flow-panel-actRoom"
										ui:field="activity">
										<g:Label>I'm moving in the room:</g:Label>
										<g:RadioButton name="Aktivitaet" formValue="sitzend">sitting</g:RadioButton>
										<g:RadioButton name="Aktivitaet" formValue="stehend">standing</g:RadioButton>
										<g:RadioButton name="Aktivitaet " formValue="leichte Aktivitaet">light
											activity</g:RadioButton>
										<g:RadioButton name="Aktivitaet" formValue="mittlere Aktivitaet">middle
											activity</g:RadioButton>
										<g:RadioButton name="Aktivitaet" formValue="koerperliche Arbeit">manuel
											work</g:RadioButton>
									</g:FlowPanel>
									<g:FlowPanel styleName="flow-panel flow-panel-feeling"
										ui:field="feeling">
										<g:Label>Today I feel:</g:Label>
										<g:RadioButton name="Tagesverfassung" formValue="hervorragend">excellent</g:RadioButton>
										<g:RadioButton name="Tagesverfassung" formValue="gesund">healthy</g:RadioButton>
										<g:RadioButton name="Tagesverfassung" formValue="leicht unwohl">slightly
											unwell</g:RadioButton>
										<g:RadioButton name="Tagesverfassung" formValue="unwohl">unwell</g:RadioButton>
										<g:RadioButton name="Tagesverfassung" formValue="krank">ill</g:RadioButton>
									</g:FlowPanel>
								</g:FlowPanel>

							</myd:content>
						</myd:GeneralDragWidget>
					</myd:draggable>

					<myd:draggable>
						<myd:GeneralDragWidget ui:field="drag4"
							styleName="dragWidget dWidget-uid-person">
							<myd:header>
								<g:HorizontalPanel styleName="dragWidget-horizontal-panel">
									<g:Label styleName="label-icon-summary"></g:Label>
									<g:Label styleName="label-tag-summary label-person-summary">Summary</g:Label>
								</g:HorizontalPanel>
							</myd:header>
							<myd:content>
								<g:FlowPanel>

									<g:FlowPanel styleName="flow-panel flow-panel-content flow-panel-summary">

										<g:FlowPanel styleName="flow-panel" ui:field="summary">
											<g:FlowPanel styleName="flow-panel-table">


												<g:FlowPanel styleName="flow-panel-tr">
													<g:Label styleName="label label-icon-air-small">Air movement</g:Label>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">ok</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-ok"></g:Label>
													</g:FlowPanel>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">neutral</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-ok"></g:Label>
													</g:FlowPanel>
												</g:FlowPanel>

												<g:FlowPanel styleName="flow-panel-tr">
													<g:Label styleName="label label-icon-temp-small">Air temperature</g:Label>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">neutral</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-ok"></g:Label>
													</g:FlowPanel>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">light</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-ok"></g:Label>
													</g:FlowPanel>
												</g:FlowPanel>


												<g:FlowPanel styleName="flow-panel-tr">
													<g:Label styleName="label label-icon-act-small">Personal activity</g:Label>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">middle activity</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-grey"></g:Label>
													</g:FlowPanel>
													<g:FlowPanel styleName="flow-panel flow-panel-subtable">
														<g:Label styleName="label-chosen-text">slightly unwell</g:Label>
														<g:Label styleName="label-icon-status label-icon-green-grey"></g:Label>
													</g:FlowPanel>
												</g:FlowPanel>

												<g:HorizontalPanel styleName="horizontal-panel horizontal-panel-save">
													<g:Button ui:field="save" styleName="saveButton">Save</g:Button>
												</g:HorizontalPanel>

											</g:FlowPanel>
										</g:FlowPanel>

									</g:FlowPanel>
								</g:FlowPanel>
							</myd:content>
						</myd:GeneralDragWidget>
					</myd:draggable>
				</myd:GeneralDropWidget>


			
		
	</g:AbsolutePanel>

		</g:FormPanel>
</ui:UiBinder>