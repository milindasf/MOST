<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.dropTextBoxFrame {
			padding: 0px 0px 0px 0px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.dropTextBoxFrame}" width="200px"
		height="30px">
		<g:TextBox ui:field="textBox" width="200px" height="30px">enter
			value or drop data point</g:TextBox>
	</g:FlowPanel>
</ui:UiBinder> 