<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HorizontalPanel>
		<g:AbsolutePanel styleName="vertical-panel vertical-panel-search">
			<g:Label ui:field="labelflowpanelSearch"
				styleName="label label-livechart label-livechart-search">Search</g:Label>
			<g:FlowPanel styleName="flow-panel flow-panel-search">
				<g:AbsolutePanel styleName="block-suggest-btn">
					<g:SuggestBox styleName="suggest-box" popupStyleName="te;test;tes"
						ui:field="sugbox" />
					<g:Button text="Search" styleName="btn-search" ui:field="searchbutton" />
					<g:Button text="Select all" styleName="btn-select-all"
						ui:field="selectallbutton" />
				</g:AbsolutePanel>
				<g:AbsolutePanel styleName="check-box-list">
					<g:AbsolutePanel styleName="column-cb column-right">
						<g:CheckBox name="searchArea" formValue="Temperature"
							styleName="check-box" ui:field="chkboxtemp" value="true">Temperature</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="Humidity"
							styleName="check-box" ui:field="chkboxhum" value="true">Humidity</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="AirQuality"
							styleName="check-box" ui:field="chkboxair" value="true">Air Quality (CO2, VOC)</g:CheckBox>
					</g:AbsolutePanel>
					<g:AbsolutePanel styleName="column-cb column-middle">
						<g:CheckBox name="searchArea" formValue="State"
							styleName="check-box" ui:field="chkboxstate" value="true">State (contact)</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="Meter"
							styleName="check-box" ui:field="chkboxmeter" value="true">Meter</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="Power"
							styleName="check-box" ui:field="chkboxpower" value="true">Power</g:CheckBox>
					</g:AbsolutePanel>
					<g:AbsolutePanel styleName="column-cb column-left">
						<g:CheckBox name="searchArea" formValue="Occupancy"
							styleName="check-box" ui:field="chkboxocc" value="true">Occupancy</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="Brightness"
							styleName="check-box" ui:field="chkboxbright" value="true">Brightness</g:CheckBox>
						<g:CheckBox name="searchArea" formValue="Other"
							styleName="check-box" ui:field="chkboxother" value="true">Other</g:CheckBox>
					</g:AbsolutePanel>
					

				</g:AbsolutePanel>
			</g:FlowPanel>
		</g:AbsolutePanel>


		<g:AbsolutePanel styleName="vertical-panel vertical-panel-datapoint">
			<g:Label ui:field="labelflowpanel"
				styleName="label label-livechart label-livechart-datapoint">Data point output</g:Label>
			<g:FlowPanel ui:field="contentArea" styleName="flow-panel flow-panel-datapoint">
			</g:FlowPanel>
		</g:AbsolutePanel>
	</g:HorizontalPanel>
</ui:UiBinder> 